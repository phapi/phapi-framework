{
  "name": "ahinko/phapi",
  "description": "Phapi is a PHP based framework aiming at simplifying API development and a the same time being fast and small and not include functionality that others to better.",
  "keywords": [
    "api", "framework", "phapi"
  ],
  "type": "framework",
  "version": "1.1.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "https://github.com/ahinko/phapi",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "psr/log": "1.*",
    "willdurand/negotiation": "1.*",
    "symfony/yaml": "2.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.4.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\": "src/Phapi/",
      "Phapi\\Tests\\": "tests/"
    }
  }
}