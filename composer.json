{
  "name": "phapi/phapi-framework",
  "description": "Phapi is a PHP based framework aiming at rapid and simplified API development as well as focusing at performance and keeping the code base small and simple.",
  "keywords": [
    "api", "framework", "phapi"
  ],
  "type": "framework",
  "version": "2.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "https://github.com/phapi/phapi-framework",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phapi/contract": "1.*",
    "phapi/exception": "1.*",
    "phapi/log": "1.*",
    "phapi/http": "1.*",
    "phapi/di": "1.*",
    "phapi/pipeline": "1.*",
    "phapi/cache-nullcache": "1.*",
    "phapi/middleware-courier": "1.*",
    "phapi/middleware-route": "1.*",
    "phapi/middleware-content-negotiation": "1.*",
    "phapi/middleware-postbox": "1.*",
    "phapi/middleware-uuid": "1.*",
    "phapi/middleware-mistake": "1.*",
    "phapi/middleware-method-override": "1.*",
    "phapi/serializer-json": "1.*",
    "phapi/endpoint": "1.*"
  },
  "replace": {
    "ahinko/phapi": "self.version"
  }
}
